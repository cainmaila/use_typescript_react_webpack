{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "files": [
        "app/my_reducer.ts",
        "app/myclass2.ts",
        "app/mySignal.ts",
        "server/web.ts",
        "typings/express/express.d.ts",
        "typings/history/history.d.ts",
        "typings/jquery/jquery.d.ts",
        "typings/js-signals/js-signals.d.ts",
        "typings/mime/mime.d.ts",
        "typings/node/node.d.ts",
        "typings/object-assign.d.ts",
        "typings/react-redux/react-redux.d.ts",
        "typings/react-router/history.d.ts",
        "typings/react-router/react-router.d.ts",
        "typings/react/react-dom.d.ts",
        "typings/react/react.d.ts",
        "typings/redux-actions.d.ts",
        "typings/redux/redux.d.ts",
        "typings/serve-static/serve-static.d.ts",
        "typings/tsd.d.ts",
        "app/cainui.tsx",
        "app/component.tsx",
        "app/component/LoginView.tsx",
        "app/component/RegView.tsx",
        "app/main.tsx",
        "app/my_item.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
